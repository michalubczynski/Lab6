        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="391*"/>
            <ColumnDefinition Width="409*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="lstBoxKategorie" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="207,143,405,232" Grid.ColumnSpan="2"/>
